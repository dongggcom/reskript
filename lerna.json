{
  "packages": ["packages/*", "showcase/*"],
  "version": "5.7.2",
  "npmClient": "yarn",
  "exact": true,
  "command": {
    "version": {
      "allowBranch": ["master"],
      "ignoreChanges": [
        "**/*.md",
        "**/example/**",
        "**/__tests__/**",
        "**/.eslintrc.cjs",
        "**/jest.config.js",
        "**/tsconfig.json"
      ]
    },
    "publish": {
      "message": "chore(release): publish %s"
    }
  }
}
